// Azure Resource Graph Explorer
// Discover Public Blob
resources
| where type =~ 'Microsoft.Storage/storageAccounts'
| extend allowBlobPublicAccess = parse_json(properties).allowBlobPublicAccess
| extend enabledBlob = parse_json(properties).enabled
| extend TLSver = parse_json(properties).minimumTlsVersion
| where allowBlobPublicAccess == "true"
| project name, resourceGroup, allowBlobPublicAccess, enabledBlob, TLSver
